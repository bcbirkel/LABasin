 simulation_velocity_out       = 0
 
 ###############################################################################
 # Output Plane Information. Coordinates in the planeinputcoords file should be
 # in coordinates x-lat, y-lon,z-depth. print_rate refers to the simulation dT.
 ###############################################################################
 number_output_planes        = 7
 output_planes_print_rate    = 5
 forces_buffer_size = 100000000
