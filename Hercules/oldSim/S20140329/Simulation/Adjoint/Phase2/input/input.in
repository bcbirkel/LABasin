 ###############################################################################
 #                                                                             #
 # physics.in: Physical quantities input file to CMU FEM toolchain.            #
 #             Lines start with # are comments. Empty lines are ignored.       #
 #                                                                             #
 ###############################################################################

 ###############################################################################
 # domain.in: Region dimention specification. Domain surface coorners are in 
 # coordinates (lon,lat) following this order: left-bottom,left-top,rigth-top,
 # and rigth-bottom.
 ###############################################################################
 region_origin_latitude_deg  = 0.0
 region_origin_longitude_deg = 0.0
 region_depth_shallow_m      = 0.0
 region_length_east_m        = 475e3
 region_length_north_m       = 760e3
 region_depth_deep_m         = 95e3
 domain_surface_corners =
  0.0		0.0
  0.0		760e3
  475e3    	760e3
  475e3 	0.0
  
 ###############################################################################
 # effective region of the model the rest will be meshed using moho properties
 # then the correct properties will be assigned
 ###############################################################################
 lat_u   = 760e3
 lat_l   = 0.0
 lon_u   = 475e3
 lon_l   = 0.0
 depth_u = 95e3
 depth_l = 0.0
 region_azimuth_leftface_deg = 0
 type_of_damping   =  mass
 source_directory        =/home/scec-00/birkel/S20140329/Simulation/Adjoint/Phase2/input/
 source_directory_output =/home/scec-00/birkel/S20140329/Simulation/Adjoint/Phase2/output/forces/

 ###############################################################################
 # numerical.in: Numerical parameters that control FEM simulation.
 #               Lines start with a # are comments. Empty lines are ignored.
 ###############################################################################
 monitor_file =/home/scec-00/birkel/S20140329/Simulation/Adjoint/Phase2/output/monitor.mon
 simulation_wave_max_freq_hz   = 0.275
 simulation_start_time_sec     = 0
 simulation_end_time_sec       = 301
 simulation_delta_time_sec     = 0.05
 simulation_node_per_wavelength = 10
 simulation_shear_velocity_min  = 0.01
 simulation_output_rate         = 10000000
 the_threshold_damping    = 0.05
 the_threshold_Vp_over_Vs = 3
 do_damping_statistics    = 0
 
 ###############################################################################
 # Database parameters
 ###############################################################################
 database_path=/home/scec-00/birkel/S20140329/Simulation/Database/

 ###############################################################################
 # Checkpointing parameters: use_checkpoint   0 - no or 1 -yes
 ###############################################################################
 checkpointing_rate = 100000
 use_checkpoint     = 0
 checkpoint_path    =/home/scec-00/birkel/S20140329/Simulation/Adjoint/Phase2/output/
 
 ###############################################################################
 # Output Information. Set to 1 if the results are needed. 0, otherwise.
 ###############################################################################
